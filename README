# usage:
brew install git neovim pgrep pkill ag jq automake # etc.
git clone git@github.com:murkey/dot_files.git $HOME/code/dot_files
ln -s $HOME/code/dot_files/.gitmodules .
ln -s $HOME/code/dot_files/.ignore .
ln -s $HOME/code/dot_files/.profile .
ln -s $HOME/code/dot_files/.zsh .
ln -s $HOME/code/dot_files/.zshrc .
mkdir -p $HOME/.config/nvim
ln -s $HOME/code/dot_files/.config/nvim/init.vim $HOME/.config/nvim
curl -fLo ~/.local/share/nvim/site/autoload/plug.vim --create-dirs https://raw.githubusercontent.com/junegunn/vim-plug/master/plug.vim
